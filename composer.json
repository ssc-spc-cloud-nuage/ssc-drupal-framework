    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3"
    },
    "config": {
        "secure-http": false,
        "prestissimo": {
            "maxConnections": 10,
            "minConnections": 5
        },
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "oomphinc/composer-installers-extender": true,
            "drupal/console-extend-plugin": true,
            "wikimedia/composer-merge-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://git.drupalcode.org/issue/form_mode_manager-3297262.git"
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "composer",
            "url": "https://drupalwxt.github.io/composer-extdeps/"
        },
        {
            "type": "package",
            "package": {
                "name": "drupaloverride/entity_field_condition",
                "version": "dev-1",
                "type": "drupal-module",
                "dist": {
                    "type": "zip",
                    "url": "https://ftp.drupal.org/files/projects/entity_field_condition-8.x-1.3.zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "drupaloverride/htmlmail",
                "version": "dev-1",
                "type": "drupal-module",
                "dist": {
                    "type": "zip",
                    "url": "https://git.drupalcode.org/api/v4/projects/project%2Fhtmlmail/repository/archive.zip?sha=dbd771f3"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "drupaloverride/twig_extensions",
                "version": "dev-1",
                "type": "drupal-module",
                "dist": {
                    "type": "zip",
                    "url": "https://git.drupalcode.org/api/v4/projects/project%2Ftwig_extensions/repository/archive.zip?sha=10b6a1a6"
                }
            }
        }
    ],
    "scripts": {
        "post-root-package-install": [
            "DrupalWxT\\WxT\\ScriptHandler::postCreateProject"
        ],
        "quick-start": [
            "composer install"
        ]
    },
    "extra": {
        "merge-plugin": {
            "include": [
                "html/modules/contrib/ckwordcount/composer.libraries.json"
            ]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "html/"
            }
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "html/core": [
                "type:drupal-core"
            ],
            "html/libraries/{$name}": [
                "type:drupal-console-library",
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "html/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "html/profiles/{$name}": [
                "type:drupal-profile"
            ],
            "html/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "composer-exit-on-patch-failure": true,
        "enable-patching": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches-file": "composer.patches.json",
        "patches-ignore": {
            "drupalwxt/wxt": {
                "drupal/diff": {
                    "Enter drupal/diff patch #3004705 description here": "https://www.drupal.org/files/issues/2018-12-03/ModerationState-error-of-translated-node-in-revision-overview-page-3004705-4.patch",
                    "Enter drupal/diff patch #3228798 description here": "https://www.drupal.org/files/issues/2021-08-19/diff-a11y-3228798-10.patch",
                    "Enter drupal/diff patch #2844337 description here": "https://www.drupal.org/files/issues/diff-remove-thumbnails-errors-2844337-17.patch",
                    "Enter drupal/diff patch #2834253 description here": "https://www.drupal.org/files/issues/2021-08-19/diff-missing-column-headers-2834253-15.patch",
                    "Enter drupal/diff patch #3183380 description here": "https://www.drupal.org/files/issues/2020-11-19/3183380-add-compare-revisions.patch"
                },
                "drupal/entity_embed": {
                    "3272732 - CKE5 compatibility": "https://www.drupal.org/files/issues/2023-06-19/entity_embed-3272732-93.patch"
                },
                "drupal/webform": {
                    "Enter drupal/webform patch #3205860 description here": "https://www.drupal.org/files/issues/2021-03-26/webform_clientside_validation-3205860-2.patch"
                },
                "drupal/fontawesome": {
                    "3071758 - Fix multiple icons issue": "https://git.drupalcode.org/project/fontawesome/-/merge_requests/4.diff"
                },
                "drupal/entity_browser": {
                    "3350169 - Fix PHP 8.2 deprecation issue with WidgetSelectorBase-class":
                    "https://www.drupal.org/files/issues/2023-06-15/3350169-8_0.patch"
                },
                "drupal/password_policy": {
                    "Enter drupal/password_policy patch #3362201 description here":
                    "https://www.drupal.org/files/issues/2023-05-23/password_policy-d10.patch"
                },
                "drupal/migrate_plus": {
                    "3404491 - [PHP 8.2] Call to deprecated function utf8_encode() at ./src/Plugin/migrate_plus/data_parser/Json.php Line 42":
                    "https://www.drupal.org/files/issues/2023-11-27/migrate-plus-php82-warning.patch"
                }
            }
        }
    },
    "autoload": {
        "classmap": [
            "scripts/ScriptHandler.php"
        ]
    }
}
